<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net7.0</TargetFrameworks>
		<Description>Provide Http client channel implementation for NetRpc.</Description>
		<SignAssembly>true</SignAssembly>
		<Version>7.0.33</Version>
		<Authors>TK</Authors>
		<PackageProjectUrl>https://github.com/xtxk110/NetRpc1</PackageProjectUrl>
		<RepositoryUrl>https://github.com/xtxk110/NetRpc1</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<AssemblyName>NetRpc.Http.Client.TK</AssemblyName>
	</PropertyGroup>
	<ItemGroup>
	  <None Include="..\..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="7.0.0" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\NetRpc\NetRpc.csproj" />
	</ItemGroup>
</Project>